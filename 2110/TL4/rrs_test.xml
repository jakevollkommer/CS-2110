<test-suite>
    <test-case>
        <name>REVERSE_SUM(HEAD1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-subr>
                <name>REVERSE_SUM</name>
                <stack>xF000</stack>
                <r5>xCAFE</r5>
                <r7>x8000</r7>
                <params>x4000</params>
            </test-subr>
        </input>
        <output>
            <test-stdout condition="equals" points = "1"><value>3 3 12 14 17 22 </value></test-stdout>
            <test-subr>
                <answer>22</answer>
                <locals>22</locals>
                <calls>
                    <call><name>REVERSE_SUM</name><params>x4004</params></call>
                    <call><name>PRINT</name><params>22</params></call>
                </calls>
                <points>
                    <answer>1</answer>
                    <params>1</params>
                    <r7>1</r7>
                    <r6>1</r6>
                    <r5>1</r5>
                    <calls>1</calls>
                    <read-answer>1</read-answer>
                </points>
                <deductions-per-mistake>1</deductions-per-mistake>
            </test-subr>
        </output>
    </test-case>
