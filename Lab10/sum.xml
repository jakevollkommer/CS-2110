<?xml version="1.0"?>
<test-suite>    
    <test-case>
        <name>sum(2,3) = 5</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value><address>X</address><value>2</value></test-value>
            <test-value><address>Y</address><value>3</value></test-value>
        </input>
        <output>
            <test-stdout points = "25"><value>The sum is: 5</value></test-stdout>
        </output>
    </test-case>
